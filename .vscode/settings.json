{
  "material-icon-theme.files.associations": {
      "Procfile.preprocess": "heroku",
      "*.pm": "document",
      "*.p": "template",
  },
  "liveServer.settings.port": 7070,
  "files.associations": {
    "*.css": "tailwindcss",
    "*.html.p": "html",
    "Profile.*": "procfile"
  },
  "tailwindCSS.includeLanguages": {
    "racket": "txt",
    "pollen-markup": "txt",
    "pollen-template-html": "html",
  },
  "tailwindCSS.experimental.classRegex": [
    // "tailwind\\s*[\"'](.*)[\"']",
    // "class\\s*[\"'](.*)[\"']",
    "class\\s\"(.*)\""
  ],
  "[racket]": {
    "editor.rulers": [88],
    "inlineFold.regex": "class\\s*[\"'](.*?)[\"']",
    "inlineFold.unfoldedOpacity": 1,
    "inlineFold.regexFlags": "g",
    "inlineFold.regexGroup": 1,
    "inlineFold.maskChar": "◦◦◦"
  },
  "[pollen-markup]": {
    "inlineFold.regex": "uid\\s*[\"'](.*?)[\"']",
    "inlineFold.unfoldedOpacity": 1,
    "inlineFold.regexFlags": "g",
    "inlineFold.regexGroup": 1,
    "inlineFold.maskChar": "◦◦◦"
  },
  "[pollen-template-html]": {
    "inlineFold.regex": "class\\s*=\\s*[\"'](.*)[\"']",
    "inlineFold.unfoldedOpacity": 1,
    "inlineFold.regexFlags": "g",
    "inlineFold.regexGroup": 1,
    "inlineFold.maskChar": "◦◦◦"
  },
  "files.exclude": {
    "**/__pycache__": true,
    ".venv": true,
    "compiled": true,
    "rkt/compiled": true,
    "node_modules": true,
    "pollen/compiled": true,
  },
  "excalidraw.image": {
    "exportScale": 1,
    "exportBackground": false,
    "exportWithDarkMode": false
  },
  // this is needed for the racket language server to work on a headless server
  "magicRacket.languageServer.command": "xvfb-run",
  "magicRacket.languageServer.arguments": [
      "--auto-servernum",
      "racket",
      "--lib",
      "racket-langserver"
  ],
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
      {
          "text": "TODO:",
          "color": "white",
          "backgroundColor": "#865C67",
      }
  ],
}
